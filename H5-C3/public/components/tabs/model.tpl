<div class="tab_wrap">
          <div class="tab_titles">
              {{#each title}}
                  <div class="tab_title">
                  {{this}}
                  </div>
              {{/each}}
          </div>

          <div class="tab_contents">
             {{#each content}}
                  <div class="tab_content">
                  {{this}}
                  </div>
              {{/each}}
          </div>

      </div>